{
  "name": "polis-client-admin",
  "version": "0.0.1",
  "repository": "github:pol-is/polisClientAdmin",
  "description": "Polis Admin Console",
  "engines": {
    "node": "11.15.0"
  },
  "scripts": {
    "analyze": "razzle build --type=spa --analyze",
    "build": "npm run build:razzle",
    "build:razzle": "razzle build --type=spa",
    "deploy": "npm run deploy:preprod",
    "deploy:preprod": "razzle build --type=spa --deploy",
    "deploy:prod": "razzle build --type=spa --deploy --prod",
    "start": "razzle start --type=spa",
    "start:prod": "serve -s build/public",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "razzle test --env=jsdom"
  },
  "author": "Colin Megill",
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "babel-eslint": "^10.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "compression-webpack-plugin": "^4.0.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-config-prettier-standard": "^3.0.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-standard": "^4.0.1",
    "event-hooks-webpack-plugin": "^2.1.7",
    "glob": "^7.1.2",
    "lodash-webpack-plugin": "^0.11.5",
    "prettier": "^2.1.2",
    "prettier-config-standard": "^1.0.1",
    "razzle": "^3.1.6",
    "razzle-plugin-mdx": "^3.1.6",
    "serve": "^11.3.2",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-s3-plugin": "^1.0.3"
  },
  "dependencies": {
    "d3-scale": "^3.2.3",
    "jquery": "^2.1.4",
    "lodash": "^4.17.20",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-easy-emoji": "^1.4.0",
    "react-redux": "7.2.1",
    "react-router-dom": "^5.2.0",
    "redux": "^3.0.2",
    "redux-thunk": "^2.3.0",
    "theme-ui": "^0.3.1",
    "victory": "^34.3.11"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": "src/**/*.js",
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
